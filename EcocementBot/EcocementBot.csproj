    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>dotnet-EcocementBot-ecd5d794-7b3c-40a1-9a67-e2275054fbcd</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="9.0.6" />
